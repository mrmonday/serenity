[[ -z $DC ]] && DC='dmd'
[[ -z $DEBUG ]] && DEBUG=1
ARGS="-L-lsqlite3 -L-lfcgi -version=EnableSqliteDb -version=EnableFastCGIBackend"
(( $DEBUG == 0 )) && ARGS="-O -inline $ARGS"
(( $DEBUG == 1 )) && ARGS="-g -debug -unittest $ARGS"
if [[ $DC = 'gdmd' ]]; then
    OD='-od'
    OF='-of'
elif [[ $DC = 'dmd' ]]; then
    ARGS=$(echo "-L-ldl $ARGS" | perl -pe 's/-g/-gc/g')
    OD='-od'
    OF='-of'
else
    OD='-od='
    OF='-of='
fi
# This should be executable and mutate itself

